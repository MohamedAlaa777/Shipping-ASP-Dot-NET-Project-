    <script>
    $(document).ready(function () {
        $("form.steps").validate({
            errorClass: "is-invalid",
            validClass: "is-valid",
            errorElement: "span",
            errorPlacement: function (error, element) {
                error.addClass("text-danger small");
                if (element.parent(".input-comment").length) {
                    error.insertAfter(element.parent());
                } else {
                    error.insertAfter(element);
                }
            },
            rules: {
                // Sender
                Sendercountry: { required: true }, // matches <select name="Sendercountry">
                SenderName: { required: true, maxlength: 50 },
                Contact: { required: true, minlength: 5 },
                Address: { required: true },
                PostalCode: { required: true, digits: true },
                SenderCityId: { required: true },
                Email: { required: true, email: true },
                Phone: { required: true, digits: true, minlength: 8 },

                // Receiver
                ReciverCountry: { required: true },
                ReciverName: { required: true, maxlength: 50 },
                ReciverContact: { required: true },
                ReciverAddress: { required: true },
                ReciverPostalCode: { required: true, digits: true },
                ReciverCity: { required: true },
                ReciverEmail: { required: true, email: true },
                ReciverPhone: { required: true, digits: true, minlength: 8 },

                // Shipment package
                Width: { required: true, number: true, min: 0.1, max: 1000 },
                Height: { required: true, number: true, min: 0.1, max: 1000 },
                Length: { required: true, number: true, min: 0.1, max: 1000 },
                Weight: { required: true, number: true, min: 0.1, max: 1000 },
                PackageValue: { required: true, number: true, min: 1 },
                ShippingRate: { required: true, number: true, min: 0 },

                // Payment & subscription
                PaymentMethodId: { required: true },
                ShippingTypeId: { required: true }, // from resource "ShippingTypes"
                UserSubscriptionId: { required: false }
            },
            messages: {
                // Sender
                Sendercountry: "@Shipping.CountryName",
                SenderName: "@Shipping.FirstNameRequired",
                Contact: "@Shipping.FirstNameRequired",
                Address: "@Shipping.LastNameRequired",
                PostalCode: "@Shipping.InvalidPhone",
                SenderCityId: "@Shipping.CountryName",
                Email: {
                    required: "@Shipping.EmailRequired",
                    email: "@Shipping.InvalidEmail"
                },
                Phone: {
                    required: "@Shipping.PhoneRequired",
                    minlength: "@Shipping.InvalidPhone"
                },

                // Receiver
                ReciverCountry: "@Shipping.CountryName",
                ReciverName: "@Shipping.FirstNameRequired",
                ReciverContact: "@Shipping.FirstNameRequired",
                ReciverAddress: "@Shipping.LastNameRequired",
                ReciverPostalCode: "@Shipping.InvalidPhone",
                ReciverCity: "@Shipping.CountryName",
                ReciverEmail: {
                    required: "@Shipping.EmailRequired",
                    email: "@Shipping.InvalidEmail"
                },
                ReciverPhone: {
                    required: "@Shipping.PhoneRequired",
                    minlength: "@Shipping.InvalidPhone"
                },

                // Shipment package
                Width: "@Shipping.FactorRange",
                Height: "@Shipping.FactorRange",
                Length: "@Shipping.FactorRange",
                Weight: "@Shipping.FactorRange",
                PackageValue: "@Shipping.FactorRequired",
                ShippingRate: "@Shipping.FactorRange",

                // Payment & subscription
                PaymentMethodId: "@Shipping.RoleRequired",
                ShippingTypeId: "@Shipping.ShippingTypes"
            }
        });

        // Prevent moving to next step if form is invalid
        $(".next").click(function () {
            if (!$("form.steps").valid()) {
                return false;
            }
        });
    });
    </script>





